<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Dorey Smoke Test Suite">
    <listeners>
        <listener class-name="selenium.ReportingListener" />
    </listeners>
    <parameter name="browserParam" value="Chrome"></parameter>
    <parameter name="locationParam" value="GRID"></parameter>
    <test name="Dorey Smoke Test">
        <classes>
            <class name="selenium.DoreyTest" />
        </classes>
    </test>
</suite>