<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Dorey Smoke" parallel="tests" thread-count="4" data-provider-thread-count="5">
    <listeners>
        <listener class-name="selenium.ReportingListener" />
    </listeners>
    <test name="Dorey Test - Chrome">
        <parameter name="browserId" value="1" />
        <parameter name="browserName" value="Chrome" />
        <parameter name="location" value="GRID" />
        <classes>
            <class name="selenium.DoreyTest" />
        </classes>
    </test>
